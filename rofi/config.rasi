@theme "rosepine"

configuration {
modes: "window,drun,run";
show-icons: true;
icon-theme: "win11";
drun-display-format: "{name}";
window-format: "{w} {c}";
window-thumbnail: false;
  timeout {
      action: "kb-cancel";
      delay:  0;
  }
  filebrowser {
      directories-first: true;
      sorting-method: "name";
  }
}
